{
  "manifest_version": 3,
  "name": "XML Sitemap Parser",
  "version": "1.0",
  "description": "Extract URLs from sitemaps",
  "permissions": ["activeTab", "storage"],
  "host_permissions": ["http://127.0.0.1:8000/*"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/icon16.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}
